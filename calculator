num1=float(input("enter 1st num"))
num2=float(input("enter 2nd num"))
operation=input("enter operation(+,-,*,/)")
if operation=='+':
  print("result"num1+num2)
elif operation=='-':
  print("result"num1-num2)
elif operation=='*':
  print("result"num1*num2)
elif operation=='/':
  print("result"num1/num2)
else :
  print ("invalis result")  
